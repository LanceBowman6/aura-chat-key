#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/dist/cli/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/dist/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/dist/cli/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/dist/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules/ts-generator/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/ts-generator@0.1.1/node_modules:/home/excnies/smartcontractdev/aura-chat-key/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-generator/dist/cli/run.js" "$@"
else
  exec node  "$basedir/../ts-generator/dist/cli/run.js" "$@"
fi
